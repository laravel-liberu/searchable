{
    "name": "laravel-liberu/searchable",
    "description": "Searchable dependency for Laravel Enso",
    "keywords": [
        "laravel-liberu",
        "searchable"
    ],
    "homepage": "https://github.com/laravel-liberu/searchable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/filters": "^2.0",
        "laravel-liberu/migrator": "^2.1",
        "laravel-liberu/permissions": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Searchable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\Searchable\\AppServiceProvider",
                "LaravelLiberu\\Searchable\\SearchServiceProvider"
            ],
            "aliases": {
                "Search": "LaravelLiberu\\Searchable\\Facades\\Search"
            }
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
